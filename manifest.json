{
  "manifest_version": 2,

  "name": "Tabber",

  "description": "This extension improves tab navigation.",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],

  "browser_action": {
    "default_popup": "popup.html"
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
