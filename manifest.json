{
  "manifest_version": 2,

  "name": "Tabber",

  "description": "This extension improves tab navigation.",
  "version": "1.0",

  "permissions": [
    "tabs",
    "http://access.alchemyapi.com/*"
  ],

  "browser_action": {
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["jquery.js", "background.js"]
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "commands": {
    "_execute_browser_action": {
       "suggested_key": {
         "default": "Alt+T",
         "mac": "MacCtrl+T"
       }
     }
  }
}
